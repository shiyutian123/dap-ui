dal-demo\",\n  data() {\n    return {\n      title: \"模态框标题\",\n      visible: false,\n      size: undefined,\n      btns: ['large', 'medium', 'small']\n    };\n  },\n  methods: {\n    /**\n     * @description: 确定点击事件\n     * @param {MouseEvent} e - 鼠标事件\n     * @return: \n     */\n    handleOk(e) {\n      this.visible = false;\n    },\n    /**\n     * @description: 取消点击事件\n     * @param {MouseEvent} e - 鼠标事件\n     * @return: \n     */\n    handleCancel(e) {\n      this.visible = false;\n    },\n    onClickBtn(size) {\n      this.size = size;\n      this.visible = true;\n    }\n  }\n};\n</script>\n"]}]}